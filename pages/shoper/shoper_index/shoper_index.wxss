/* 首页轮播 */

.swiper-box {
  width: 100%;
  height: 320rpx;
}

.swiper-box image {
  width: 100%;
  height: 320rpx;
  display: block;
}

.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 5rpx;
}

.swiper-box .wx-swiper-dot {
  width: 24rpx;
  display: inline-flex;
  height: 4rpx;
  margin-left: 2rpx;
  justify-content: space-between;
}

.swiper-box .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(51, 51, 51, 1);
  border-radius: 8rpx;
}

.swiper-box .wx-swiper-dot-active::before {
  background: rgba(255, 32, 99, 1);
}

