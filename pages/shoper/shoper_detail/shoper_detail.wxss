.slide_box {
  width: 100%;
  background: #f2f2f2;
  height: auto;
  overflow: hidden;
  position: absolute;
}

.slide_swiper {
  width: 100%;
  height: 750rpx;
}

.slide_swiper image {
  width: 100%;
  height: 750rpx;
  display: block;
}

/* 轮播点 */

.slide_swiper .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 2rpx;
}

.slide_swiper .wx-swiper-dot {
  width: 12rpx;
  display: inline-flex;
  height: 12rpx;
  margin-left: 10rpx;
  justify-content: space-between;
  /* border-radius: 50%; */
}

.slide_swiper .wx-swiper-dot::before {
  /* border:1rpx solid #fff; */
  content: '';
  flex-grow: 1;
  background: rgba(0, 0, 0, 0.5);
  /* box-sizing: border-box;  */
  border-radius: 50%;
}

.slide_swiper .wx-swiper-dot-active::before {
  background: rgba(255, 32, 99, 1);
  /* box-sizing: border-box;  */
  border-radius: 50%;
}

