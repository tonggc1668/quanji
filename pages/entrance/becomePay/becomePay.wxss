page {
  width: 100%;
  height: 100%;
  background-color: #ededed;
}

.view_root {
  margin-top: 2rpx;
  background-color: white;
  width: 100%;
  height: 100%;
  position: relative;
}

.image_success {
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
  padding-top: 170rpx;
  display: block;
}

.view_text {
  height: 50rpx;
  width: 100%;
  margin-top: 34rpx;
  font-size: 36rpx;
  text-align: center;
}

.view_change {
  width: 200rpx;
  height: 25rpx;
  margin: 82rpx auto;
}

.image_back {
  width: 24rpx;
  height: 24rpx;
  margin-left: 6rpx;
}

.text {
  font-size: 28rpx;
  color: #5a6fff;
}

.view_bottom {
  position: absolute;
  left: 0;
  bottom: 0;
  background-color: #ff2063;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: white;
  font-size: 32rpx;
}
