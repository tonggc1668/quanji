.swiper-tab {
  height: 60rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 60rpx;
  background-color: #F7F7F7;
  white-space: nowrap;
  text-align: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
}

.swiper-tab-list {
  font-size: 32rpx;
  display: inline-block;
  width: 33.3%;
  color: #FF2063;
}

.on {
  color: white;
  background-color: #FF2063;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.scoll-h {
  height: 100%;
  width: 100%;
  background-color: white;
  margin-top: 60rpx; 
   /* overflow: hidden ; */
  /* overflow: hidden;*/

}





.loading{  
  padding: 10rpx;  
  text-align: center;  
}  
.loading:before{  
  display: inline-block;  
  margin-right: 5rpx;  
  vertical-align: middle;  
  content: '';  
  width: 40rpx;  
  height: 40rpx;  
  /* background: url("../../../images/mine/loading.png") no-repeat;   */
  background-size: contain;  
  animation: rotate 1s linear infinite;  
}  
.loading.complete:before{  
  display: none;  
}