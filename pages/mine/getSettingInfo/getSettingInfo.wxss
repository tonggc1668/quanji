page {
  width: 100%;
  height: 100%;
  background-color: white;
}

.view_null_root {
  background-color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bg {
  width: 100%;
  height: 700rpx;
}

.text_small {
  color: #cac9ce;
  font: 20rpx;
  margin-top: 10rpx;
}

.view_text {
  width: 363rpx;
  display: block;
  margin: 0 auto;
  margin-top: 36rpx;
  font-size: 28rpx;
  color: #a6a6a6;
  text-align: center;
}

.view_go {
  width: 590rpx;
  height: 70rpx;
