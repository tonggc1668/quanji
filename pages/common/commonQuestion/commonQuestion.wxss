page {
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
  overflow: auto !important;
}

.container {
  margin-bottom:30rpx;
}

/* 圈集小店是什么 */

.first-container {
  width: 100%;
  background-color: white;
}

.top-container {
  width: 100%;
  height: 110rpx;
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 20rpx;
}

.first-title {
  float: left;
  margin-left: 20rpx;
  line-height: 110rpx;
  font-size: 28rpx;
  color: #333;
}

.first-arrow {
  float: right;
  width: 24rpx;
  height: 24rpx;
  margin-right: 19rpx;
  margin-top: 49rpx;
}

.first-detail {
  color: #7B7B7B;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 20rpx;
  font-size: 24rpx;
}

.arrow-flod {
  color: red;
}

.arrow-extend {
  color: orange;
}

.a {
  color: red;
}

.b {
  color: orange;
}
